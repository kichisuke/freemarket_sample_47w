.container
  = render 'shared/header'
  .breadcrumb
    %ul
      %li
        - breadcrumb :logout
        = render "layouts/breadcrumbs"
  .contents.clearfix
    = render partial: "shared/mypage-side"
    .mypage
      .mypage__logout
        = link_to 'ログアウト', destroy_user_session_path, method: :DELETE, class: 'mypage__logout__btn'

  = render 'shared/banner'
  = render 'shared/footer'
  = render 'shared/footer-sellBtn'
